<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="net.tavda.Controllers.TorchDamagingController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label text="d="/>
    <TextField fx:id="d">
        <VBox.margin>
            <Insets bottom="10.0"/>
        </VBox.margin>
    </TextField>
    <Label text="H="/>
    <TextField fx:id="H">
        <VBox.margin>
            <Insets bottom="10.0"/>
        </VBox.margin>
    </TextField>
    <Button mnemonicParsing="false" onAction="#calc" text="Выполнить расчёт">
        <VBox.margin>
            <Insets bottom="10.0"/>
        </VBox.margin>
    </Button>
    <Label text="Результаты"/>
    <TextArea fx:id="result" prefHeight="200.0" prefWidth="200.0"/>
</VBox>
